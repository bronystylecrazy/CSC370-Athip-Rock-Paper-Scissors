  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.2.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "solid-app-router": "^0.3.0",
    "solid-js": "^1.3.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vite-plugin-resolve": "^1.4.4",
    "vite-plugin-solid": "^2.2.5"
  },
  "env": {
