{
  "name": "athip-electron",
  "productName": "Athip Rock Paper Scissors",
  "private": true,
  "version": "1.0.0",
  "description": "Vite SolidJS Electron boilerplate.",
  "author": "ch99q <64793a1a@gmail.com>",
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
